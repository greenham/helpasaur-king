{
  "name": "helpa-discord",
  "version": "1.12.0",
  "main": "./dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/index.js",
    "start:dev": "nodemon --watch src --ext ts --exec 'npm run build && node ./dist/index.js'",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "deploy-commands": "node ./src/deploy-commands.js"
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "express": "^4.19.2",
    "helpa-api-client": "file:../lib/helpa-api-client",
    "ms": "^2.1.3",
    "node-schedule": "^2.1.1",
    "socket.io-client": "^4.5.4"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.2.1",
    "@types/node-schedule": "^2.1.8",
    "nodemon": "^3.0.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
